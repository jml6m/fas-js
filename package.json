{
  "name": "fas-js",
  "description": "Finate State Automata JS Solutions",
  "version": "1.1.8",
  "source": "src/modules.js",
  "main": "lib/modules.js",
  "umd:main": "lib/modules.umd.js",
  "module": "lib/modules.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jml6m/fas-js.git"
  },
  "scripts": {
    "build": "babel src/ -d lib/ && microbundle build --strict --sourcemap false -f es,umd",
    "prebuild": "npm run clean",
    "clean": "rimraf lib/*",
    "prepublishOnly": "cross-env NODE_ENV=prod npm run build && npm run flow check",
    "flow": "flow",
    "test": "npm run build && cross-env NODE_ENV=test nyc --check-coverage --lines 90 mocha --require @babel/register ./test/**/*.spec.js"
  },
  "precommit": "test",
  "author": "Joseph Lewkovich",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "flow-bin": "^0.91.0",
    "microbundle": "^0.9.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "temp-dir": "./coverage/.nyc_output"
  },
  "bugs": {
    "url": "https://github.com/jml6m/fas-js/issues"
  },
  "homepage": "https://github.com/jml6m/fas-js#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "state-machine",
    "fsa",
    "finite",
    "state",
    "automata",
    "automaton",
    "dfa",
    "ndfa",
    "turing"
  ],
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "chalk": "^2.4.2"
  },
  "files": [
    "lib/"
  ]
}
